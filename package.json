{
  "name": "tuplatainforma",
  "type": "module",
  "version": "5.5.1",
  "packageManager": "pnpm@10.30.2",
  "scripts": {
    "dev": "astro dev",
    "build": "pnpm run check:frontmatter && astro check && astro build && pnpm run check:toc-i18n && pnpm run check:editorial-guard && pagefind --site dist && cp -r dist/pagefind public/",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "check": "pnpm run check:frontmatter && astro check",
    "test": "vitest run",
    "test:watch": "vitest",
    "check:frontmatter": "node scripts/check-frontmatter.mjs",
    "routes:snapshot": "node scripts/snapshot-routes.mjs",
    "routes:compare": "node scripts/compare-routes.mjs docs/reports/routes_snapshot_before.json docs/reports/routes_snapshot_after.json",
    "check:routes": "node scripts/snapshot-routes.mjs --out docs/reports/routes_snapshot_after.json && pnpm run routes:compare",
    "check:boundaries": "node scripts/check-domain-boundaries.mjs",
    "check:toc-i18n": "node scripts/check-toc-i18n.mjs",
    "check:editorial-guard": "node scripts/check-editorial-guard.mjs",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.0",
    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.6",
    "dayjs": "^1.11.19",
    "lodash.kebabcase": "^4.1.1",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.18.3",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.2",
    "@pagefind/default-ui": "^1.4.0",
    "@shikijs/transformers": "^3.20.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/lodash.kebabcase": "^4.1.9",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "globals": "^17.3.0",
    "pagefind": "^1.4.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.18"
  }
}
